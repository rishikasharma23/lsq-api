{
    "type": "object",
    "title": "Root Schema",
    "required": [
        "leadName",
        "leadEmail",
        "leadContact",
        "leadBirthday"
    ],
    "properties": {
        "leadName": {
            "type": "string",
            "title": "The leadName Schema",
            "examples": [
                "sahill"
            ]
        },
        "leadEmail": {
            "type": "string",
            "title": "The leadEmail Schema",
            "examples": [
                "kush@gmail.com"
            ]
        },
        "leadContact": {
            "type": "string",
            "title": "The leadContact Schema",
            "examples": [
                "8929714332"
            ]
        },
        "leadBirthday": {
            "type": "string",
            "title": "The leadBirthday Schema",
            "examples": [
                "23/12/2000"
            ]
        },
        "product": {
            "type": "string",
            "enum": [
                "A",
                "B",
                "C"
            ],
            "description": "ABC product"
        },
        "nationality": {
            "type": "string",
            "enum": [
                "Indian",
                "American",
                "Canadian",
                "British",
                "French"
            ],
            "description": "User's nationality."
        },
        "smokingStatus": {
            "type": "string",
            "enum": [
                "Smoker",
                "Non-Smoker"
            ],
            "description": "Specifies if the person is a Smoker or Non-Smoker."
        }
    },
    "examples": [
        {
            "leadName": "sahil",
            "leadEmail": "kush@gmail.com",
            "leadContact": "8929714332",
            "leadBirthday": "23/12/2000"
        }
    ]
}